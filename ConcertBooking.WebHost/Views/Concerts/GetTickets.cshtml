@model IEnumerable<ConcertBooking.WebHost.ViewModels.ConcertViewModels.DashboardViewModel>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConcertName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SeatNumbers)
            </th>
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConcertName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SeatNumbers)
            </td>
            
        </tr>
}
    </tbody>
</table>
